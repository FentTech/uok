{
  "buildCommand": "npm run build:client",
  "outputDirectory": "dist/spa",
  "nodeVersion": "18.x",
  "routes": [
    {
      "src": "^/assets/.*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
